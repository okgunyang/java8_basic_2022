package sec2;

import java.util.Scanner;

public class Condition2 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("성명 : ");
		String name = sc.next();
		System.out.print("\n점수 : ");
		int jum = sc.nextInt();
		String pan="";
		if(jum>=90){
			pan="A";
		} else if(jum>=80){
			pan="B";
		} else if(jum>=70){
			pan="C";
		}
	}
}

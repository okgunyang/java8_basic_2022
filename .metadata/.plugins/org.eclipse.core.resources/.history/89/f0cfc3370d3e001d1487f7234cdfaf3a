package sec4;

public abstract class Student {
	String schoolname;
	String bun;
	String name;
	int jum1;
	int jum2;
	int jum3;
	
	abstract int computeTot(int jum1, int jum2, int jum3);
	abstract double computeAvg(int jum1, int jum2, int jum3);
	String computeHak(int jum1, int jum2, int jum3){
		
	}
}
